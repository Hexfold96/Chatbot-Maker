Response:",          "content": "    HTTP/1.1 200 OK\n\n{\n        \"data\": {\n            \"responses\": [\n                null\n            ],\n            \"utterances\": [\n                \"Good morning\",\n                \"早安\"\n            ],\n            \"_id\": \"5b41c24c4b47cffe4c2dc241\",\n            \"intent\": \"Good_Morning\",\n            \"__v\": 0\n        }\n    }",          "type": "json"        }      ]    },    "version": "0.0.0",    "filename": "./intent/intent.controller.js",    "groupTitle": "Intent"  },  {    "type": "get",    "url": "/intent/:intentId",    "title": "Get Intent",    "name": "Get_Intent",    "group": "Intent",    "description": "<p>get the detail of the intent</p>",    "parameter": {      "fields": {        "Parameter": [          {            "group": "Parameter",            "type": "String",            "optional": false,            "field": "intentId",            "description": "<p>the id of the intent</p>"          }        ]      }    },    "success": {      "examples": [        {          "title": "Success-Response:",          "content": "    HTTP/1.1 200 OK\n\n{\n        \"data\": {\n            \"responses\": [\n                \"你好\"\n            ],\n            \"utterances\": [\n                \"你好\",\n                \"Hello\"\n            ],\n            \"_id\": \"5b40d9c0b9c1b6f5956c63a7\",\n            \"intent\": \"Hello\",\n            \"__v\": 0\n        }\n    }",          "type": "json"        }      ]    },    "version": "0.0.0",    "filename": "./intent/intent.controller.js",    "groupTitle": "Intent"  },  {    "type": "get",    "url": "/intent",    "title": "Get intents",    "name": "Get_Intents",    "group": "Intent",    "description": "<p>get the created intent and respone according to the limit and offset</p>",    "parameter": {      "fields": {        "Parameter": [          {            "group": "Parameter",            "type": "String", 