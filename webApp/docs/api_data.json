[  {    "type": "post",    "url": "/bot",    "title": "Messaging bot",    "name": "Messaging_bot",    "group": "Bot",    "description": "<p>speak with the bot and reply with the response from database</p>",    "parameter": {      "fields": {        "Parameter": [          {            "group": "Parameter",            "type": "String",            "optional": false,            "field": "message",            "description": "<p>the input from user</p>"          }        ]      }    },    "success": {      "examples": [        {          "title": "Success-Response:",          "content": "HTTP/1.1 200 OK\n{\n     \"response\": \"Hello world!\"\n}",          "type": "json"        }      ]    },    "version": "0.0.0",    "filename": "./bot/bot.controller.js",    "groupTitle": "Bot"  },  {    "type": "post",    "url": "/bot/train",    "title": "Train Bot",    "name": "Train_Bot",    "group": "Bot",    "description": "<p>Train the bot with defined intents</p>",    "success": {      "examples": [        {          "title": "Success-Response:",          "content": "HTTP/1.1 200 OK",          "type": "String"        }      ]    },    "version": "0.0.0",    "filename": "./bot/bot.controller.js",    "groupTitle": "Bot"  },  {    "type": "get",    "url": "/config",    "title": "Get the bot config",    "name": "Get_Config",    "group": "Config",    "description": "<p>get the bot's config including the fallback, threshold and botOn status</p>",    "success": {      "examples": [        {          "title": "Success-Response:",          "content": "   HTTP/1.1 200 OK\n{\n       \"data\": {\n           \"_id\": \"5b40e6d04f9ce5f84de4ac94\",\n           \"fallback\": \"唔好意思, 我唔係太明你講乜 wo\",\n           \"threshold\": 0.7,\n           \"botOn\": true,\n           \"createdAt\": \"2018-07-07T16:14:08.790Z\",\n           \"updatedAt\": \"2018-07-07T16:14:08.790Z\",\n           \"__v\": 0\n       }\n}",          "type": "json"        }      ]    },    "version": "0.0.0",    "filename": "./config/config.controller.js",    "groupTitle": "Config"  },  {    "type": "post",    "url": "/config",    "title": "Set the bot config",    "name": "Set_Config",    "group": "Config",    "description": "<p>set the bot's config including the fallback, threshold and botOn status</p>",    "parameter": {      "fields": {        "Parameter": [          {            "group": "Parameter",            "type": "String",            "optional": false,            "field": "fallback",            "description": "<p>The message reply to the user when fallback occur</p>"          },          {            "group": "Parameter",            "type": "Number",            "optional": false,            "field": "threshold",            "description": "<p>If the prediction score is lower than the threshold, then it would goes into fallback</p>"          },          {            "group": "Parameter",            "type": "Boolean",            "optional": false,            "field": "botOn",            "description": "<p>Variable that control the onOff of the bot</p>"          }        ]      }    },    "success": {      "examples": [        {          "title": "Success-Response:",          "content": "   HTTP/1.1 200 OK\n{\n       \"data\": {\n           \"_id\": \"5b40e6d04f9ce5f84de4ac94\",\n           \"fallback\": \"唔好意思, 我唔係太明你講乜 wo\",\n           \"threshold\": 0.7,\n           \"botOn\": true,\n           \"createdAt\": \"2018-07-07T16:14:08.790Z\",\n           \"updatedAt\": \"2018-07-07T16:14:08.790Z\",\n           \"__v\": 0\n       }\n}",          "type": "json"        }      ]    },    "version": "0.0.0",    "filename": "./config/config.controller.js",    "groupTitle": "Config"  },  {    "type": "post",    "url": "/intent",    "title": "Create intent",    "name": "Create_intent",    "group": "Intent",    "description": "<p>create the intent with label, utterances